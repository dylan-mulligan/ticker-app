{
  "name": "ticker-ts-react-electron",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron.js",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@tabler/icons-react": "^3.31.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "dayjs": "^1.11.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.2",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.3",
    "web-vitals": "^2.1.4",
    "vite": "^4.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "vite-plugin-electron": "^0.10.0"
  },
  "scripts": {
    "react-start": "vite",
    "electron-start": "set \"ELECTRON_START_URL=http://localhost:5173\"&& electron .",
    "start": "concurrently \"npm run react-start\" \"npm run electron-start\"",
    "build": "vite build public --outDir build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "dist": "npm run build && npm run build:electron && electron-builder --win",
    "serve": "electron .",
    "test": "vitest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^22.15.2",
    "concurrently": "^9.1.2",
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  }
}
